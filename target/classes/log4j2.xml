<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info" name="biyh.httpserver" packages="">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %-5p %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <!--比level低级别的日志不会被记录-->
        <Root level="INFO">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>